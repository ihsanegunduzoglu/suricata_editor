{
  "name": "suricata-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-resizable-panels": "^3.0.5",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "^15.6.3",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently -n CLIENT,NODE_API,PYTHON_MAIN,PYTHON_MITRE -c green,blue,magenta,cyan \"npm run dev:react\" \"npm run dev:node\" \"npm run dev:python:main\" \"npm run dev:python:mitre\"",

    "start": "react-scripts start", 
    "dev:react": "npm run start",

    "dev:node": "npm --prefix server run dev",

    "dev:python:main": "./backend/venv/bin/python backend/app.py",

    "dev:python:mitre": "./mitre-api/venv/bin/python mitre-api/app.py",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:4100",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
